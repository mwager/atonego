{
    "passfail": false,
    "node": true,
    "browser": true,
    "es5": false,
    "esnext": false,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 2,
    "predef": ["angular", "define", "__app_config__", "setTimeout", "prompt", "confirm", "log", "expect", "describe", "it", "before", "beforeEach", "after", "afterEach", "sinon", "device", "navigator", "__"],
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "maxdepth": 4,
    "maxcomplexity": 15,
    "maxlen": 120,
    "white": false,
    "asi": true
}
